{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "CustomerManagementAPI": {
        "ClusterId": "CustomerManagementAPICluster",
        "Match": {
          "Path": "/CustomerManagementAPI/{**remainder}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**remainder}"
          }
        ]
      },
      "VehicleManagementAPI": {
        "ClusterId": "VehicleManagementAPICluster",
        "Match": {
          "Path": "/VehicleManagementAPI/{**remainder}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**remainder}"
          }
        ]
      },
      "WorkshopManagementAPI": {
        "ClusterId": "WorkshopManagementAPICluster",
        "Match": {
          "Path": "/WorkshopManagementAPI/{**remainder}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**remainder}"
          }
        ]
      }
    },
    "Clusters": {
      "CustomerManagementAPICluster": {
        "Destinations": {
          "Primary": {
            "Address": "http://customermanagementapi:5100"
          }
        }
      },
      "VehicleManagementAPICluster": {
        "Destinations": {
          "Primary": {
            "Address": "http://vehiclemanagementapi:5000"
          }
        }
      },
      "WorkshopManagementAPICluster": {
        "Destinations": {
          "Primary": {
            "Address": "http://workshopmanagementapi:5200"
          }
        }
      }
    }
  }
}
