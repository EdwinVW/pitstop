{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://logserver:5341" }
      }
    ],
    "Properties": {
      "Application": "WebApp"
    }
  },
  "APIServiceLocations": {
    "CustomerManagementAPI": "apigateway:6000/CustomerManagementAPI",
    "VehicleManagementAPI": "apigateway:6000/VehicleManagementAPI",
    "WorkshopManagementAPI": "apigateway:6000/WorkshopManagementAPI"
  }
}
